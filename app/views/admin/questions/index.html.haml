%table{:border => "1"}
  %tr
    %th Question
    %th Field
  - for q in @questions
    - if q.field_name=="text_field"
      %tr
        %td= q.question_name
        %td= text_field_tag q.question_name
    - elsif q.field_name=="check_box_tag"
      %tr
        %td= q.question_name
        %td= check_box_tag q.question_name, value = q.question_name, checked = false, options = {}
    - elsif q.field_name=="radio_button_tag"
      %tr
        %td= q.question_name
        %td= radio_button_tag  q.question_name , :checked=>false
    - else
      %tr
        %td= q.question_name
        %td= select_tag q.question_name, value = nil, options = {}
%span{:style => "background-color:white;color:red;"}= link_to "New Question" , new_admin_question_path
%span{:style => "background-color:white;color:blue;margin-left:50px"}= link_to "Answer" , new_admin_answer_path
%span{:style => "background-color:white;color:blue;margin-left:50px"}= link_to "User Form" , new_user_form_path
